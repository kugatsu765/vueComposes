{
  "name": "@kugatsu/vue-composes",
  "description": "Some composition function to help & share",
  "version": "1.0.10",
  "author": "Romain GABEL",
  "license": "ISC",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:lib": "npx unbuild",
    "publish:lib": "npm publish --access public",
    "publish:dry": "npm publish --dry-run",
    "serve": "vite preview",
    "test": "jest"
  },
  "keywords": [
    "vue3",
    "vuejs",
    "compositionApi"
  ],
  "dependencies": {
    "vue": "^3.2.16",
    "@kugatsu/utilities": "^1.0.2"
  },
  "peerDependencies": {},
  "devDependencies": {
    "unbuild": "^0.5.13",
    "@types/jest": "^27.0.3",
    "@vitejs/plugin-vue": "^1.9.3",
    "jest": "^27.4.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.3",
    "vite": "^2.6.4",
    "vue-tsc": "^0.3.0"
  }
}